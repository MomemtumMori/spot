class A {
    a;
    f(A:this, x) { return this.a + x; }
};

class B {
    b;
    f(B:this, x) { return this.b * x; }
};

class AB : A, B {
	c;
	ctor(AB:this) {
		this.c = 30;
	}
	dtor(AB:this) {
		this.c = 0;
	}
	g(AB:this, x) {
		return this.c % (
			(A:this).f(x) +
			(B:this).f(x)
			);
	}

public OnPluginStart() {
    new A:myA;
    myA.a = 10;
    new Bool:shouldBe12 = myA.f(2) == 12;
    PrintToServer("myA.f(2) == 12 => %b", shouldBe12);
    
    new B:myB;
    myB.b = 5;
    new Bool:shouldBe10 = myB.f(2) == 10;
    PrintToServer("myB.f(2) == 10 => %b", shouldBe10);
    
    new AB:myAB;
    new Bool:shouldBe8 = myAB.g(2) == 8;
    PrintToServer("myAB.g(2) == 8 => %b", shouldBe8);
}